{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://mistakia.github.io/react-table/schema/export/export-options.json",
  "title": "ExportOptions",
  "type": "object",
  "properties": {
    "default_filename": {
      "type": "string",
      "default": "table-export",
      "description": "Default filename for exports"
    },
    "include_metadata": {
      "type": "boolean",
      "default": true,
      "description": "Whether to include metadata in exports"
    },
    "use_zero_for_null": {
      "type": "boolean",
      "default": false,
      "description": "Whether to use zero values for null data in exports"
    },
    "supported_formats": {
      "type": "array",
      "items": { "type": "string" },
      "default": ["csv", "json", "markdown", "html", "tsv"],
      "description": "Supported export formats"
    }
  }
}
